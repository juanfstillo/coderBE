

<div class="container p-4">
    <div class="row justify-content-center align-items-center">
        <div class="col-md4 mx-auto">

            <div class="card">

                <div class="card-body">
                    <h1>

                        {{#each food}}

                        <div>
                            <h3>{{this.name}}</h3>
                            <p>{{this.price}}</p>
                        </div>
                        {{/each}}
                    </h1>
                </div>
            </div>
        </div>
    </div>
</div>